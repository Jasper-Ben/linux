/dts-v1/;

#include "sc58x.dtsi"

/ {
	model = "ADI sc584-ezkit";
	compatible = "adi,sc584-ezkit", "adi,sc58x";

	aliases {
	};

	memory@89000000 {
		device_type = "memory";
		reg = <0x89000000 0x7000000>;
	};

	scb {
		button0: button@0 {
			compatible = "adi,button-led";
			button_gpio = <15>;
			led_gpio = <65>;
		};
		button1: button@1 {
			compatible = "adi,button-led";
			button_gpio = <16>;
			led_gpio = <66>;
		};
		button2: button@2 {
			compatible = "adi,button-led";
			button_gpio = <47>;
			led_gpio = <67>;
		};
		
	};
};
