/dts-v1/;

#include "sc57x.dtsi"

/ {
	model = "ADI sc573-ezkit";
	compatible = "adi,sc573-ezkit", "adi,sc57x";

	aliases {
		/* serial2 = &uart2; */
	};

	memory@80000000 {
		device_type = "memory";
		reg = <0x80000000 0xF000000>;
	};

	scb {
		button0: button@0 {
			compatible = "adi,button-led";
			button_gpio = <40>;
			led_gpio = <77>;
		};
		button1: button@1 {
			compatible = "adi,button-led";
			button_gpio = <41>;
			led_gpio = <9>;
		};
		button2: button@2 {
			compatible = "adi,button-led";
			button_gpio = <42>;
			led_gpio = <65>;
		};

	};
};
